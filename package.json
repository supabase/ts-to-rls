{
  "name": "ts-to-rls",
  "version": "0.1.0",
  "description": "TypeScript DSL for defining Row Level Security policies - works in Node.js, Deno, Bun, and browsers",
  "packageManager": "pnpm@10.28.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./dist/*.d.ts": "./dist/*.d.ts",
    "./dist/bundle.d.ts": "./dist/bundle.d.ts"
  },
  "sideEffects": false,
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc && dts-bundle-generator -o dist/bundle.d.ts src/index.ts",
    "docs": "typedoc",
    "lint": "eslint src tests",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:integration": "VITEST_INTEGRATION=1 vitest run",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "supabase:status": "supabase status",
    "test:integration:full": "bash scripts/run-integration-tests.sh",
    "prepublishOnly": "pnpm build && pnpm test"
  },
  "keywords": [
    "rls",
    "row-level-security",
    "postgresql",
    "supabase",
    "dsl",
    "typescript",
    "database",
    "security",
    "policies",
    "sql",
    "node",
    "deno",
    "bun",
    "browser"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/supabase/ts-to-rls.git"
  },
  "bugs": {
    "url": "https://github.com/supabase/ts-to-rls/issues"
  },
  "homepage": "https://github.com/supabase/ts-to-rls#readme",
  "engines": {
    "node": ">=14.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^20.19.33",
    "@types/pg": "^8.16.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.55.0",
    "@typescript-eslint/parser": "^8.55.0",
    "@vitest/coverage-v8": "^4.0.18",
    "dts-bundle-generator": "^9.5.1",
    "eslint": "^9.39.2",
    "pg": "^8.18.0",
    "typedoc": "^0.28.16",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.55.0",
    "uuid": "^10.0.0",
    "vitest": "^4.0.18"
  }
}
