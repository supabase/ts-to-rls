{
  "name": "rowguard",
  "version": "0.2.0",
  "private": false,
  "description": "TypeScript DSL for defining Row Level Security policies - works in Node.js, Deno, Bun, and browsers",
  "packageManager": "pnpm@10.28.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./dist/*.d.ts": "./dist/*.d.ts",
    "./dist/bundle.d.ts": "./dist/bundle.d.ts"
  },
  "sideEffects": false,
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc && dts-bundle-generator -o dist/bundle.d.ts src/index.ts",
    "docs": "typedoc",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint src tests",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:integration": "VITEST_INTEGRATION=1 vitest run",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "supabase:status": "supabase status",
    "test:integration:full": "bash scripts/run-integration-tests.sh",
    "prepublishOnly": "pnpm build && pnpm test",
    "supabase:types": "supabase gen types typescript --local > demo/src/database.types.ts",
    "supabase:setup": "supabase start && pnpm supabase:types",
    "supabase:reset": "supabase db reset && pnpm supabase:types",
    "demo:dev": "cd demo && pnpm dev",
    "demo:dev:full": "pnpm supabase:setup && pnpm demo:dev",
    "demo:build": "pnpm build && cd demo && pnpm build",
    "demo:preview": "cd demo && pnpm preview"
  },
  "keywords": [
    "rls",
    "row-level-security",
    "postgresql",
    "supabase",
    "dsl",
    "typescript",
    "database",
    "security",
    "policies",
    "sql",
    "node",
    "deno",
    "bun",
    "browser"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/supabase-community/rowguard.git"
  },
  "bugs": {
    "url": "https://github.com/supabase-community/rowguard/issues"
  },
  "homepage": "https://github.com/supabase-community/rowguard#readme",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^20.19.33",
    "@types/pg": "^8.16.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.55.0",
    "@typescript-eslint/parser": "^8.55.0",
    "@vitest/coverage-v8": "^4.0.18",
    "dts-bundle-generator": "^9.5.1",
    "eslint": "^9.39.2",
    "pg": "^8.18.0",
    "prettier": "3.8.1",
    "typedoc": "^0.28.16",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.55.0",
    "uuid": "^10.0.0",
    "vitest": "^4.0.18"
  }
}
